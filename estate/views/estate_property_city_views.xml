<odoo>

    <!-- List View -->
     <record id="view_estate_property_city_list" model="ir.ui.view">
        <field name="name">estate.property.city.list</field>
        <field name="model">estate.city</field>
        <field name="arch" type="xml">
            <list string="City List">
                <field name="name"/>
            </list>
        </field>
    </record>
    <!-- Form View -->
    <record id="view_estate_property_tag_form" model="ir.ui.view">
        <field name="name">estate.property.city.form</field>
        <field name="model">estate.city</field>
        <field name="arch" type="xml">
            <form string="City Lists">
                <sheet>
                    <field name="name"/>
                    <field name="township_ids"/>
                </sheet>
            </form>
        </field>
    </record>
    <!-- Action -->
    <record id="estate_property_city_action" model="ir.actions.act_window">
        <field name="name">City Lists</field>
        <field name="res_model">estate.city</field>
        <field name="view_mode">list,form</field>
    </record>
    <!-- Menu -->
    <menuitem id="estate_property_city_menu" 
            parent="estate_property_settings"
            action="estate_property_city_action"/>

</odoo>